pipeline{
    agent any
    
    environment{
        PATH = "/opt/apache-maven-3.9.9/bin:$PATH"
    }
    stages{
        stage("Git Checkout"){
            steps{
                git credentialsId: 'Password', url: 'https://github.com/Nagesh05021993/Java-HelloWorld.git'
            }
        }
        stage("Maven Build"){
            steps{
                sh "mvn clean package"

        echo "Hello"
            }
        }
      
    }
}
